<!--This page grabs data from the CwaUserMetrics and CwaGroupMetrics tables (based on who's logged in) and puts it into tables -->

    <table cellspacing = 10 >
        <tr>
            <th>ID</th>
            <th>CPU Time</th>
            <th>CPU Time Per Job</th>
            <th>Wall Clock</th>
            <th>Disk Usage (Home)</th>
            <th>Disk Usage (Work)</th>
            <th>Total Jobs</th>
            <th>Job Time</th>
            
        </tr>
            <% if !CwaUserMetrics.find_all_by_user_id(User.current.id).blank?%>
            <% @cwa_user_metrics = CwaUserMetrics.find_all_by_user_id(User.current.id) %>
            <% @cwa_user_metrics.each do |x| %>

        <tr>
            <td><center><%= x.user_id %></center></td>
            <td><center><%= x.cpu_time %></center></td>
            <td><center><%= x.cpu_time_job %></center></td>
            <td><center><%= x.tot_wallclock %></center></td>
            <td><center><%= x.disk_usage_home %></center></td>
            <td><center><%= x.disk_usage_work %></center></td>
            <td><center><%= x.tot_jobs %></center></td>
            <td><center><%= x.average_job_time %></center></td>
            
        </tr>

            <% end %><% end %>

    </table>

